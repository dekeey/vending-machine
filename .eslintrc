{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-class-assign": 0,
    "no-debugger": 1,
    "no-console": 1,
    "new-cap": 0,
    "strict": 0,
    "no-undef": 1,
    "no-underscore-dangle": 0,
    "no-use-before-define": 2,
    "no-unused-vars": 1,
    "eol-last": 0,
    "quotes": [1, "single"],
    "jsx-quotes": [1, "prefer-single"],
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/prop-types": 1
  }
}